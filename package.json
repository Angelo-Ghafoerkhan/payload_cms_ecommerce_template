{
  "name": "csn",
  "version": "1.0.0",
  "description": "Website template for Payload",
  "license": "MIT",
  "type": "module",
  "tsx": {
    "ignore": [
      "\\.css$",
      "\\.(scss|sass|less)$"
    ]
  },
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "postbuild": "next-sitemap --config next-sitemap.config.cjs",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "dev:prod": "cross-env NODE_OPTIONS=--no-deprecation rm -rf .next && pnpm build && pnpm start",
    "generate:importmap": "cross-env TSX_IGNORE=\\.css$ payload generate:importMap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "generate:types:all": "cross-env NODE_OPTIONS=--no-deprecation pnpm exec payload generate:types",
    "ii": "cross-env NODE_OPTIONS=--no-deprecation pnpm --ignore-workspace install",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "lint:fix": "cross-env NODE_OPTIONS=--no-deprecation next lint --fix",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "reinstall": "cross-env NODE_OPTIONS=--no-deprecation rm -rf node_modules && rm pnpm-lock.yaml && pnpm --ignore-workspace install",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.0",
    "@lexical/react": "0.28.0",
    "@lexical/selection": "0.28.0",
    "@next/third-parties": "^15.2.4",
    "@payloadcms/admin-bar": "latest",
    "@payloadcms/db-postgres": "latest",
    "@payloadcms/email-nodemailer": "^3.28.1",
    "@payloadcms/live-preview-react": "latest",
    "@payloadcms/next": "latest",
    "@payloadcms/payload-cloud": "latest",
    "@payloadcms/plugin-form-builder": "latest",
    "@payloadcms/plugin-nested-docs": "latest",
    "@payloadcms/plugin-redirects": "latest",
    "@payloadcms/plugin-search": "latest",
    "@payloadcms/plugin-seo": "latest",
    "@payloadcms/richtext-lexical": "^3.43.0",
    "@payloadcms/translations": "^3.30.0",
    "@payloadcms/ui": "latest",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tabs": "^1.1.12",
    "@types/jsonwebtoken": "^9.0.9",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "colortranslator": "^5.0.0",
    "cross-env": "^7.0.3",
    "framer-motion": "^12.11.0",
    "geist": "^1.3.0",
    "graphql": "^16.8.2",
    "jsonwebtoken": "^9.0.2",
    "lexical": "0.28.0",
    "lucide-react": "^0.378.0",
    "next": "15.2.3",
    "next-sitemap": "^4.2.3",
    "nodemailer": "^6.10.0",
    "patch-package": "^8.0.0",
    "payload": "^3.43.0",
    "postinstall-postinstall": "^2.1.0",
    "prism-react-renderer": "^2.3.1",
    "react": "19.0.0",
    "react-colorful": "^5.6.1",
    "react-dom": "19.0.0",
    "react-hook-form": "7.45.4",
    "react-infinite-logo-slider": "^1.1.4",
    "react-intersection-observer": "^9.16.0",
    "react-spinners": "^0.15.0",
    "react-toast": "^1.0.3",
    "react-toastify": "^11.0.5",
    "sharp": "0.32.6",
    "stripe": "^17.7.0",
    "tailwind-merge": "^2.3.0",
    "tailwind-scrollbar-hide": "^2.0.0",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/typography": "^0.5.13",
    "@types/escape-html": "^1.0.2",
    "@types/node": "22.5.4",
    "@types/nodemailer": "^6.4.17",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
    "autoprefixer": "^10.4.19",
    "copyfiles": "^2.4.1",
    "eslint": "^9.16.0",
    "eslint-config-next": "15.2.3",
    "postcss": "^8.4.38",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.3",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "typescript": "5.7.3"
  },
  "packageManager": "pnpm@10.12.1",
  "engines": {
    "node": "^18.20.2 || >=20.9.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp"
    ],
    "overrides": {
      "tsx": "4.20.3"
    }
  }
}
